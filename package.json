{
  "name": "mapcss",
  "version": "1.0.0",
  "description": "Parser for MapCSS",
  "main": "lib/mapcss-parser",
  "keywords": [
    "mapcss",
    "openstreetmap",
    "parser"
  ],
  "dependencies": {
    "nearley": "^2.16.0"
  },
  "devDependencies": {
    "eslint": "^5.15.1",
    "colors": "^1.3.3",
