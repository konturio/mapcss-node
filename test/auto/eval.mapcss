// @Test Use eval type conversion
way {
  width: eval(num("2") * 4);
}

// @Test Eval tag value
way {
  opacity: eval(tag("population")/100000);
}

// @Test Eval arithmetics
way {
    width: eval(1 + 2 * 3 - 4 / 5);
}

// @Test Parentheses
way {
    width: eval((1 + 2) * 3);
    width: eval(6 * (1 + 2 * 2) * 3 + 2);
}


// @Test nested functions
way {
    text: eval(tag(concat("", "name", ":ru")));
}
