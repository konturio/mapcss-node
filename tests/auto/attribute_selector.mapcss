// Exact tag value
way[highway=residential] {
}

// Tag presence
way[shop] {
}

// Tag absence
way[!shop] {
}

// Underscore in tag name
node[opening_hours] {
}

// Quoted tag value
node[name="\"Sunrise\" hotel"] {
}

// Colon in tag name without quotes
way[addr:street] {
}

// Quoted tag name
way["highway street"] {
}

// Combination of attributes
way[highway=track][!lit] {
}

// Tag value not equals
way[tunnel!=yes] {}

// Tag value lesser
way[population<10000] {}

// Tag value lesser or equals
way[population<=10000] {}

// Tag value greater
way[population>10000] {}

// Tag value greater or equals
way[population>=10000] {}

// Tag value regex match
way[name~=/^(St\/?art).*$/] {}
